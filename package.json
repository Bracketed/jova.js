{
	"name": "@bracketed/node-express-ts-template",
	"packageManager": "yarn@4.1.0",
	"version": "1.0.0",
	"repository": "https://github.com/bracketed/templates-node-typescript-express",
	"author": {
		"name": "bracketed",
		"url": "https://github.com/bracketed"
	},
	"main": "./dist/index.js",
	"type": "module",
	"imports": {
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"dev-start": "yarn tsc && yarn node ./dist/index.js",
		"start": "yarn node ./dist/index.js",
		"test": "yarn tsx watch ./src/index.ts",
		"compile": "yarn tsc",
		"compose-build": "docker compose build",
		"compose-up": "docker compose -f compose.yml up -d"
	},
	"devDependencies": {
		"@bracketed/typescript-config": "^4.4.0",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^22.5.2",
		"prettier": "^3.3.3",
		"tsx": "^4.19.0",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"@bracketed/logger": "^1.0.2",
		"@sapphire/stopwatch": "^1.5.2",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"express-rate-limit": "^7.4.0"
	}
}
