{
	"name": "@bracketed/jova.js",
	"description": "Jova.js - A semi-advanced Express.js framework by Bracketed Softworks!",
	"packageManager": "yarn@4.1.0",
	"version": "1.5.0",
	"publishConfig": {
		"access": "public"
	},
	"license": "Apache-2.0",
	"author": "@bracketed",
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/types/index.d.ts",
	"exports": {
		".": {
			"types": "./lib/types/index.d.ts",
			"require": "./lib/cjs/index.js",
			"import": "./lib/esm/index.js"
		},
		"./utilities": {
			"types": "./lib/types/utilities/index.d.ts",
			"require": "./lib/cjs/utilities/index.js",
			"import": "./lib/esm/utilities/index.js"
		},
		"./types": {
			"types": "./lib/types/types/index.d.ts",
			"require": "./lib/cjs/types/index.js",
			"import": "./lib/esm/types/index.js"
		}
	},
	"files": [
		"lib"
	],
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bracketed/jova.js.git",
		"directory": "."
	},
	"homepage": "https://jova.js.org/",
	"sideEffects": false,
	"bugs": {
		"url": "https://github.com/bracketed/jova.js/issues"
	},
	"keywords": [
		"bracketed",
		"typescript",
		"express",
		"framework",
		"http",
		"server",
		"webserver",
		"web",
		"rest",
		"router",
		"app",
		"api"
	],
	"scripts": {
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build": "yarn build:cjs && yarn build:esm",
		"build:production": "npm run build:cjs && npm run build:esm",
		"test": "yarn tsc && yarn node ./lib/example/index.test.js",
		"compile": "yarn tsc",
		"deploy": "gh-pages -d build"
	},
	"devDependencies": {
		"@bracketed/typescript-config": "^4.4.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4",
		"@types/node": "^22.7.2",
		"@types/range-parser": "^1.2.7",
		"@types/tcp-port-used": "^1.0.4",
		"depcheck": "^1.4.7",
		"jsdoc": "^4.0.3",
		"marked": "^14.1.2",
		"prettier": "^3.3.3",
		"typedoc": "^0.26.7",
		"typedoc-github-theme": "^0.1.2",
		"typedoc-github-wiki-theme": "^2.0.0",
		"typedoc-plugin-inline-sources": "^1.1.0",
		"typedoc-plugin-markdown": "^4.2.8",
		"typedoc-plugin-mdn-links": "^3.3.1",
		"typescript": "^5.6.2"
	},
	"dependencies": {
		"@bracketed/express": "^4.21.7",
		"@bracketed/logger": "^1.0.10",
		"@sapphire/stopwatch": "^1.5.2",
		"cors": "^2.8.5",
		"express": "4.21.0",
		"express-rate-limit": "^7.4.0",
		"ioredis": "^5.4.1",
		"range-parser": "^1.2.1",
		"rate-limit-redis": "^4.2.0",
		"tcp-port-used": "^1.0.2"
	}
}
