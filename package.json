{
	"name": "@bracketed/jova.js",
	"packageManager": "yarn@4.1.0",
	"version": "1.0.0",
	"publishConfig": {
		"access": "public"
	},
	"license": "Apache-2.0",
	"author": "@bracketed",
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/types/index.d.ts",
	"exports": {
		".": {
			"types": "./lib/types/index.d.ts",
			"require": "./lib/cjs/index.js",
			"import": "./lib/esm/index.js"
		}
	},
	"files": [
		"lib"
	],
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bracketed/jova.js.git",
		"directory": "."
	},
	"homepage": "https://bracketed.co.uk",
	"sideEffects": false,
	"bugs": {
		"url": "https://github.com/bracketed/node-logger/issues"
	},
	"keywords": [
		"bracketed",
		"typescript",
		"express",
		"framework",
		"http",
		"server",
		"webserver",
		"web",
		"rest",
		"router",
		"app",
		"api"
	],
	"scripts": {
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build": "yarn build:cjs && yarn build:esm",
		"build:production": "npm run build:cjs && npm run build:esm",
		"test": "yarn tsc && yarn node ./lib/example/index.test.js",
		"compile": "yarn tsc"
	},
	"devDependencies": {
		"@bracketed/typescript-config": "^4.4.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^22.5.5",
		"prettier": "^3.3.3",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2"
	},
	"dependencies": {
		"@bracketed/logger": "^1.0.10",
		"@sapphire/stopwatch": "^1.5.2",
		"axios": "^1.7.7",
		"cors": "^2.8.5",
		"express": "^4.21.0",
		"express-rate-limit": "^7.4.0",
		"ioredis": "^5.4.1",
		"rate-limit-redis": "^4.2.0"
	}
}
